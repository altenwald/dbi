{erl_opts, [
    debug_info,
    {platform_define, "R1[56]", 'OLD_QUEUE_TYPE'}
]}.

{plugins, [rebar3_hex]}.

{deps, [
    {cache, "2.2.0"},
    {eql, "0.1.2"}
]}.

{cover_enabled, true}.
{cover_export_enabled, true}.
{cover_opts, [verbose]}.
{eunit_opts, [verbose]}.

{xref_checks, [undefined_function_calls, undefined_functions,
               deprecated_function_calls, deprecated_functions]}.
